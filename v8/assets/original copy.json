{
  "params": {
    "score": 0.6,
    "requested": 10000.00,
    "pre-approved": 5000.00,
    "installment": 12
  },
  "steps": [
    {
      "condition": {
        "left": "params.requested",
        "right": "params.pre-approved",
        "operator": "greater_than"
      },
      "then": {
        "payload": {
          "credit": "params.requested",
          "credit_used": "params.pre-approved"
        },
        "steps": [
          {
            "condition": {
              "left": "params.credit",
              "right": "params.credit_used",
              "operator": "greater_than"
            },
            "then": {
              "payload": {
                "score": "params.credit - params.credit_used"
              },
              "steps": [
                {
                  "condition": {
                    "left": "steps.step1.score",
                    "right": 10,
                    "operator": "greater_than"
                  }
                },
                {
                  "condition": {
                    "left": "steps.step1.score",
                    "right": 500,
                    "operator": "greater_than"
                  }
                },
                {
                  "condition": {
                    "left": "steps.step1.score",
                    "right": 100000,
                    "operator": "less_than"
                  }
                },
                {
                  "then": {
                    "condition": {
                      "left": "steps.step1.score",
                      "right": 500,
                      "operator": "equal"
                    },
                    "then": {
                      "return": true
                    }
                  }
                }
              ]
            },
            "else": {
              "steps": [
                {
                  "score": 0
                }
              ]
            }
          }
        ]
      },
      "else": {
        "steps": [
          {
            "condition": {
              "left": "params.score",
              "right": 0.5,
              "operator": "greater_than"
            }
          },
          {
            "id": "approved",
            "payload": {
              "total": "(params.requested * 0.3) + params.pre-approved"
            }
          },
          {
            "condition": {
              "left": "steps.approved.total",
              "right": 10000.00,
              "operator": "greater_than"
            },
            "then": {
              "return": 10000.00
            },
            "else": {
              "return": "steps.approved.total"
            }
          }
        ]
      }
    },
    {
      "condition": {
        "left": "steps.step1.score",
        "right": 500,
        "operator": "greater_than"
      },
      "then": {
        "return": true
      }
    }
  ]
}